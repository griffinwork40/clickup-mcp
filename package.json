{
  "name": "@griffinwork40/clickup-mcp-server",
  "version": "1.1.6",
  "description": "MCP server for ClickUp API integration - manage tasks, projects, and workflows",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "clickup-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "griffinwork40",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/griffinwork40/clickup-mcp.git"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "mcp",
    "clickup",
    "model-context-protocol",
    "task-management",
    "project-management"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "axios": "^1.7.9",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
